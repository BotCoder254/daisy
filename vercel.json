{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "BACKEND_URL": "https://portfolio-backend-8q07.onrender.com"
    },
    "git": {
        "submodules": false
    }
} 